# Vercel Environment Variables Example
# Copy this file to .env.local for local development

# Upstash Redis (Vercel KV)
KV_URL=your_upstash_redis_url
KV_REST_API_URL=your_upstash_rest_api_url
KV_REST_API_TOKEN=your_upstash_rest_api_token
KV_REST_API_READ_ONLY_TOKEN=your_upstash_read_only_token

# JWT Secret for authentication
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Optional: Environment
NODE_ENV=development